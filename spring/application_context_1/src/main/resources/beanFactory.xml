<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="serviceA" class="spring_test.ServiceA">
        <property name="animal">
            <value>dog</value>
        </property>
    </bean>
    <bean id="serviceB" class="spring_test.ServiceB"></bean>
    <bean id="fpp" class="spring_test.MyBeanFactoryPostProcessor"></bean>
    <bean id="pp" class="spring_test.MyBeanPostProcessor"></bean>
</beans>